{
  "input": {
    "type": "console",
    "params": {
      "prompt": "You> ",
      "confirm_before_send": false,
      "allow_edit_before_send": false,
      "print_input": false
    }
  },
  "llm": {
    "type": "ollama_cloud",
    "params": {
      "host": "https://ollama.com",
      "model": "gpt-oss:120b",
      "api_key": "${OLLAMA_API_KEY}",
      "system_prompt": "master_prompts/default.txt"
    }
  },
  "output": {
    "type": "console",
    "params": {
      "prefix": "BOT"
    }
  }
}
